{
  "name": "pinepain/php-ref-lib",
  "description": "Weak-referenced data structures for PHP",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php-ref",
    "weak",
    "reference",
    "weakref",
    "weakreference",
    "weak data types",
    "data structure"
  ],
  "authors": [
    {
      "name": "Bogdan Padalko",
      "email": "pinepain@gmail.com",
      "homepage": "https://github.com/pinepain"
    }
  ],
  "require": {
    "php": "~7.0",
    "ext-ref": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.1",
    "pinepain/php-ref-stubs": "~0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Ref\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ref\\Tests\\": "./tests"
    }
  }
