{
  "name": "pinepain/php-object-maps",
  "description": "ObjectMap structures for PHP",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "map",
    "bimap",
    "object map",
    "object bimap",
    "weak object map",
    "weak object bimap"
  ],
  "authors": [
    {
      "name": "Bogdan Padalko",
      "email": "pinepain@gmail.com",
      "homepage": "https://github.com/pinepain"
    }
  ],
  "require": {
    "php": "~7.0"
  },
  "suggest": {
    "ext-ref": "Needed to support weak map variations"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "pinepain/php-ref-stubs": "~0.4.1"
  },
  "autoload": {
    "psr-4": {
      "Pinepain\\ObjectMaps\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pinepain\\ObjectMaps\\Tests\\": "./tests"
    }
  }
}
